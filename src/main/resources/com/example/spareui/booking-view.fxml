                                 <Insets top="20.0" />
                              </VBox.margin>
                           </DatePicker>
                           <DatePicker fx:id="checkoutBox" editable="false" layoutX="10.0" layoutY="10.0" onAction="#onCheckoutBoxFilled" prefHeight="32.0" prefWidth="215.0" promptText="Select Checkout" style="-fx-background-color: eaf4fd;" />
