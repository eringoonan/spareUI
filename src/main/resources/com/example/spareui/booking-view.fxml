                           <Label fx:id="totalLabel" text="Total Days:">
                              <font>
                                 <Font name="Berlin Sans FB" size="20.0" />
                              </font>
                              <VBox.margin>
                                 <Insets top="20.0" />
                              </VBox.margin>
                           </Label>
                           <Label fx:id="totalPriceLabel" layoutX="10.0" layoutY="35.0" text="£00">
                              <font>
                                 <Font name="Berlin Sans FB" size="16.0" />
                              </font>
                           </Label>
                           <Button fx:id="bookButton" mnemonicParsing="false" onAction="#onBookClick" prefHeight="40.0" prefWidth="214.0" style="-fx-background-color: #529be8; -fx-background-radius: 25;" text="Book" textFill="WHITE">
