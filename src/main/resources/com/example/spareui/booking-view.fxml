                <children>
                    <Button fx:id="menuButton" mnemonicParsing="false" onAction="#onMenuClick" prefHeight="30.0" prefWidth="164.0" style="-fx-background-color: #529be8; -fx-background-radius: 15;" text="Menu" textFill="WHITE">
