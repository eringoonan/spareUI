                           <DatePicker fx:id="checkinBox" editable="false" onAction="#onCheckinBoxFilled" prefHeight="32.0" prefWidth="215.0" promptText="Select Checkin" style="-fx-background-color: eaf4fd; -fx-background-radius: 15; -fx-border-radius: 15;">
