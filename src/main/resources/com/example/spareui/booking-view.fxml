                        </VBox.margin>
                        <graphic>
                            <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true">
                                <image>
                                    <Image url="@../../../images/blueaccounticon.png" />
                                </image>
                            </ImageView>
                        </graphic>
                        <cursor>
                            <Cursor fx:constant="HAND" />
                        </cursor>
                    </Button>
                </children>
            </VBox>
        </children>
    </GridPane>
   <VBox alignment="CENTER" fillWidth="false" spacing="10.0" style="-fx-background-color: f6faff; -fx-background-radius: 15;" AnchorPane.bottomAnchor="80.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="65.0">
